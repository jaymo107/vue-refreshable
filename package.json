{
  "name": "vue-refreshable",
  "version": "0.0.5",
  "description": "Refreshable forms.",
  "author": "JD",
  "scripts": {
    "test": "vue-cli-service test:unit",
    "build:watch": "npm run build:es -- --watch && npm run build:umd -- --watch",
    "build": "npm run build:es && npm run build:umd",
    "build:es": "rollup --config --format es --file dist/vueRefreshable.esm.js",
    "build:umd": "rollup --config --format umd --file dist/vueRefreshable.umd.js"
  },
  "main": "./dist/vueRefreshable.umd.js",
  "module": "./dist/vueRefreshable.esm.js",
  "dependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.2.2",
    "rollup": "^2.54.0",
    "rollup-plugin-terser": "^7.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "bugs": {
    "url": "https://github.com/jaymo107/vue-refreshable/issues"
  },
  "config": {
    "libName": "vue-refreshable"
  },
  "homepage": "https://github.com/jaymo107/vue-refreshable#readme",
  "keywords": [
    "vue",
    "refresh",
    "forms",
    "data",
    "localStorage",
    "persist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaymo107/vue-refreshable.git"
  }
}
